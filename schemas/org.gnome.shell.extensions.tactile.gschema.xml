<?xml version="1.0" encoding="UTF-8" ?>
<schemalist>
    <schema id="org.gnome.shell.extensions.ctile" path="/org/gnome/shell/extensions/ctile/">
        <!-- Show/hide -->
        <key name="ctile-left" type="as">
            <default><![CDATA[['<Super>Left']]]></default>
            <summary>Ctile left</summary>
        </key>
        <key name="ctile-right" type="as">
            <default><![CDATA[['<Super>Right']]]></default>
            <summary>Ctile right</summary>
        </key>
        <key name="ctile-up" type="as">
            <default><![CDATA[['<Super>Up']]]></default>
            <summary>Ctile up</summary>
        </key>
        <key name="ctile-down" type="as">
            <default><![CDATA[['<Super>Down']]]></default>
            <summary>Ctile down</summary>
	      </key>
        <key name="ctile-switch-monitor" type="as">
            <default><![CDATA[['<Super>Enter']]]></default>
            <summary>Move window to next monitor</summary>
	      </key>
    </schema>
</schemalist>
